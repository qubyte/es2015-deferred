{
  "name": "es2015-deferred",
  "version": "3.0.0",
  "description": "A tiny constructor for deferred objects. Useful for stubbing promises in tests.",
  "type": "commonjs",
  "main": "./index.umd.js",
  "jsnext:main": "index.mjs",
  "module": "index.mjs",
  "exports": {
    "require": "./index.umd.js",
    "import": "./index.mjs"
  },
  "repository": "qubyte/es2015-deferred",
  "scripts": {
    "lint": "eslint .",
    "test-cjs": "node test.js",
    "test-esm": "node --experimental-modules test.mjs",
    "build": "rollup -f umd -n Deferred index.mjs -o index.umd.js",
    "clean": "rimraf index.umd.js",
    "prepublish": "npm run build",
    "postpublish": "npm run clean"
  },
  "files": [
    "index.mjs",
    "index.umd.js"
  ],
  "keywords": [
    "defer",
    "deferred",
    "promise",
    "promises"
  ],
  "author": "Mark S. Everitt",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-qubyte": "^3.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.7.5",
    "tape": "^5.0.0"
  }
}
